{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "browsers": "> 5%",
          "node": "10"
        },
        "modules": "umd",
        "useBuiltIns": "entry"
      }
    ]
  ],
  "plugins": [
    "add-module-exports",
    "@babel/plugin-proposal-object-rest-spread"
  ]
}
